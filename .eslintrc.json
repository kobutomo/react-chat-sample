{
	"extends": [
		"react-app"
	],
	"plugins": [
		"@typescript-eslint"
	],
	"env": {
		"browser": true,
		"es6": true
	},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"sourceType": "module",
		"project": "./tsconfig.json"
	},
	"rules": {
		"@typescript-eslint/camelcase": 0,
		"semi": [
			"error",
			"always"
		],
		"no-extra-semi": "error"
	}
}